[run]
branch = True
source = py_eacopy

[report]
exclude_lines =
    if self.debug:
    pragma: no cover
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    pass
    ...
ignore_errors = True
omit =
    tests/*
    setup.py
    docs/*
    examples/*
    extern/*

[html]
directory = htmlcov
